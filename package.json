{
  "name": "ed-library",
  "version": "0.0.0",
  "description": "Easily write and share edcuational content with educational markdown aka emd!",
  "scripts": {
    "prestart": "node scripts/buildCJSModules.mjs",
    "build:index": "npx pagefind --site \"_site\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npx concurrently -n w: npm:watch:*",
    "watch:html": "npm run prestart && npx @11ty/eleventy --serve",
    "watch:css": "postcss \"11ty/_assets/style.postcss\" --verbose --base . --dir . --ext .css --config postcss.config.cjs --watch"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.8",
    "open-props": "^1.5.15",
    "postcss": "^8.4.29",
    "postcss-cli": "^10.1.0",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^12.0.1",
    "postcss-preset-env": "^9.1.3",
    "prettier": "^3.0.3"
  },
  "dependencies": {
    "@ed-components/ed-components": "^0.0.12",
    "pagefind": "^1.0.3"
  }
}